{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Proxye": {
    "Port": 9567,
    "DnsPort": 9568,
    "EnableDns": true,
    "Dns": {
      "Url": "https://dns.google/resolve",
      "BaseTtl": 3600
    },
    "Rules": [
      {
        "Pattern": ".*(youtube|googlevideo|ggpht|yt3).*",
        "Protocol": "SOCKS5",
        "Host": "127.0.0.1",
        "Port": 1080
      },
      {
        "Domains": ["2ip.io"],
        "Protocol": "HTTP",
        "Host": "127.0.0.1",
        "Port": 1080
      }
    ]
  }
}
