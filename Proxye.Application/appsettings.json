{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Proxye": {
    "Port": 9567,
    "DnsHost": "1.1.1.1",
    "Rules": [
      {
        "Pattern": ".*(youtube|googlevideo|ggpht|yt3).*",
        "Protocol": "SOCKS5",
        "Host": "127.0.0.1",
        "Port": 1080
      },
      {
        "Domains": ["2ip.io"],
        "Protocol": "HTTP",
        "Host": "127.0.0.1",
        "Port": 1080
      }
    ]
  }
}
